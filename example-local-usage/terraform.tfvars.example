# Example Terraform variables file for Graylog provider
# Copy this to terraform.tfvars and update with your actual values

graylog_endpoint    = "https://your-graylog-server.com/api"
graylog_username    = "admin"
graylog_password    = "your-secure-password"
graylog_api_version = "v1"  # Use "v1" for older Graylog versions, "v2" for newer